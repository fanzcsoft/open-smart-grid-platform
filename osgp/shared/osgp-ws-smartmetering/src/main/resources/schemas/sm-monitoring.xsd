    <xsd:complexContent>
      <xsd:extension base="common:Response">
        <xsd:sequence>
          <xsd:element maxOccurs="2" name="PowerQualityProfileDatas"
            type="tns:PowerQualityProfileData"/>
        </xsd:sequence>
      </xsd:extension>

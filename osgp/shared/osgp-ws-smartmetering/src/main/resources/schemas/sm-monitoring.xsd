          <xsd:element name="BeginDate" type="xsd:dateTime"/>
          <xsd:element name="EndDate" type="xsd:dateTime"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>


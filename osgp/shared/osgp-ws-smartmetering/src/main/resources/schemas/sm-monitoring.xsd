    <xsd:sequence maxOccurs="unbounded" minOccurs="0">
      <xsd:element name="AmrProfileStatusCodeFlag" type="common:AmrProfileStatusCodeFlag"/>
    </xsd:sequence>
  </xsd:complexType>

  <!-- gas meter actual -->
  <xsd:complexType name="PeriodicMeterReads">
    <xsd:sequence>
      <xsd:element name="LogTime" type="xsd:dateTime"/>
      <xsd:element minOccurs="0" name="ActiveEnergyImport" type="tns:MeterValue"/>
      <xsd:element minOccurs="0" name="ActiveEnergyExport" type="tns:MeterValue"/>
      <xsd:element minOccurs="0" name="ActiveEnergyImportTariffOne" type="tns:MeterValue"/>
      <xsd:element minOccurs="0" name="ActiveEnergyImportTariffTwo" type="tns:MeterValue"/>
      <xsd:element minOccurs="0" name="ActiveEnergyExportTariffOne" type="tns:MeterValue"/>
      <xsd:element minOccurs="0" name="ActiveEnergyExportTariffTwo" type="tns:MeterValue"/>
      <xsd:element minOccurs="0" name="AmrProfileStatusCode" type="tns:AmrProfileStatusCode"/>
    </xsd:sequence>
  </xsd:complexType>

  <!-- This is a special type so that ActualMeterReads is supported in bundle requests -->
  <xsd:complexType name="PeriodicMeterReadsGas">
    <xsd:sequence>
      <xsd:element name="LogTime" type="xsd:dateTime"/>
      <xsd:element name="Consumption" type="tns:MeterValue"/>
      <xsd:element name="CaptureTime" type="xsd:dateTime"/>
      <xsd:element minOccurs="0" name="AmrProfileStatusCode" type="tns:AmrProfileStatusCode"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="MeterValue">
    <xsd:simpleContent>
      <xsd:extension base="tns:Value">
        <xsd:attribute name="unit" type="common:OsgpUnitType" use="required"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="PowerQualityProfileRequestData">
    <xsd:complexContent>
      <xsd:extension base="common:Query">

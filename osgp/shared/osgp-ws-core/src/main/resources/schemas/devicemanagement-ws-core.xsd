<xsd:schema xmlns:common="http://www.opensmartgridplatform.org/schemas/common/2014/10"
  xmlns:tns="http://www.opensmartgridplatform.org/schemas/devicemanagement/2014/10"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  attributeFormDefault="unqualified" elementFormDefault="qualified"
  targetNamespace="http://www.opensmartgridplatform.org/schemas/devicemanagement/2014/10">

  <xsd:complexType name="ScheduledTask">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CreationTime"
        type="xsd:dateTime"/>
      <xsd:element minOccurs="0" name="ModificationTime"
        type="xsd:dateTime"/>
      <xsd:element minOccurs="0" name="Domain"
        type="xsd:string"/>
      <xsd:element minOccurs="0" name="DomainVersion"
        type="xsd:string"/>
      <xsd:element minOccurs="0" name="OrganisationIdentification"
        type="common:Identification"/>
      <xsd:element minOccurs="0" name="DeviceIdentification"
        type="common:Identification"/>
      <xsd:element minOccurs="0" name="MessageType"
        type="xsd:string"/>
      <xsd:element minOccurs="0" name="ScheduledTime"
        type="xsd:dateTime"/>
      <xsd:element minOccurs="0" name="Status"
        type="tns:StatusGroup"/>
      <xsd:element minOccurs="0" name="ErrorLog"
        type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Device">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="DeviceUid" type="xsd:string"/>
      <xsd:element name="DeviceIdentification" type="common:Identification"/>
      <xsd:element minOccurs="0" name="Alias" type="xsd:string"/>
      <xsd:element minOccurs="0" name="Owner" type="xsd:string"/>
      <xsd:element minOccurs="0" name="ContainerAddress" type="common:Address"/>
      <xsd:element minOccurs="0" name="GpsLatitude" type="xsd:string"/>
      <xsd:element minOccurs="0" name="GpsLongitude" type="xsd:string"/>
      <xsd:element minOccurs="0" name="NetworkAddress" type="tns:IPType"/>
      <xsd:element minOccurs="0" name="DeviceType" type="xsd:string"/>
      <xsd:element minOccurs="0" name="Activated" type="xsd:boolean"/>
      <xsd:element minOccurs="0" name="HasSchedule" type="xsd:boolean"/>
      <xsd:element minOccurs="0" name="PublicKeyPresent" type="xsd:boolean"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Organisations" type="xsd:string"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="OutputSettings"
        type="tns:DeviceOutputSetting"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Eans" type="tns:Ean"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="RelayStatuses" type="tns:RelayStatus"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DeviceAuthorizations"
        type="tns:DeviceAuthorization"/>
      <xsd:element minOccurs="0" name="InMaintenance" type="xsd:boolean"/>
      <xsd:element minOccurs="0" name="TechnicalInstallationDate" type="xsd:dateTime"/>
      <xsd:element minOccurs="0" name="DeviceModel" type="tns:DeviceModel"/>
      <xsd:element minOccurs="0" name="LightMeasurementDevice" type="tns:LightMeasurementDevice"/>
      <xsd:element minOccurs="0" name="DeviceLifecycleStatus" type="tns:DeviceLifecycleStatus"/>
      <xsd:element minOccurs="0" name="LastCommunicationTime" type="xsd:dateTime"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="UpdatedDevice">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Alias" type="xsd:string"/>
      <xsd:element minOccurs="0" name="ContainerAddress" type="common:Address"/>
      <xsd:element minOccurs="0" name="GpsLatitude" type="xsd:string"/>
      <xsd:element minOccurs="0" name="GpsLongitude" type="xsd:string"/>
      <xsd:element minOccurs="0" name="Activated" type="xsd:boolean"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="OutputSettings"
        type="tns:DeviceOutputSetting"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Eans" type="tns:Ean"/>
      <xsd:element minOccurs="0" name="TechnicalInstallationDate" type="xsd:dateTime"/>
      <xsd:element minOccurs="0" name="DeviceLifecycleStatus" type="tns:DeviceLifecycleStatus"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="LightMeasurementDevice">
    <xsd:complexContent>
      <xsd:extension base="tns:Device">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="Description" type="xsd:string"/>
          <xsd:element minOccurs="0" name="Code" type="xsd:string"/>
          <xsd:element minOccurs="0" name="Color" type="xsd:string"/>
          <xsd:element minOccurs="0" name="DigitalInput" type="xsd:short"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="RelayStatus">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Index" type="xsd:int"/>
      <xsd:element minOccurs="0" name="LastSwitchingEventState"
        type="xsd:boolean"/>
      <xsd:element minOccurs="0" name="LastSwitchingEventTime"
        type="xsd:dateTime"/>
      <xsd:element minOccurs="0" name="LastKnownState"
        type="xsd:boolean"/>
      <xsd:element minOccurs="0" name="LastKnownStateTime"
        type="xsd:dateTime"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="DevicePage">
    <xsd:sequence>
      <xsd:element name="TotalPages" type="xsd:int"/>
      <xsd:element maxOccurs="unbounded" name="Devices"
        type="tns:Device"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="DeviceFilter">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="OrganisationIdentification"
        type="common:Identification"/>
      <xsd:element minOccurs="0" name="DeviceIdentification"
        type="xsd:string"/>
      <xsd:element minOccurs="0" name="Alias" type="xsd:string"/>
      <xsd:element minOccurs="0" name="City" type="xsd:string"/>
      <xsd:element minOccurs="0" name="PostalCode"
        type="xsd:string"/>
      <xsd:element minOccurs="0" name="Street"
        type="xsd:string"/>
      <xsd:element minOccurs="0" name="Number"
        type="xsd:string"/>
      <xsd:element minOccurs="0" name="Municipality"
        type="xsd:string"/>
      <xsd:element minOccurs="0" name="DeviceType"
        type="xsd:string"/>
      <xsd:element minOccurs="0" name="Manufacturer"
        type="xsd:string"/>
      <xsd:element minOccurs="0" name="Model" type="xsd:string"/>
      <xsd:element minOccurs="0"
        name="DeviceExternalManaged" type="tns:DeviceExternalManagedFilterType"/>
      <xsd:element minOccurs="0" name="DeviceActivated"
        type="tns:DeviceActivatedFilterType"/>
      <xsd:element minOccurs="0" name="DeviceInMaintenance"
        type="tns:DeviceInMaintetanceFilterType"/>
      <xsd:element minOccurs="0" name="SortDir"
        type="xsd:string"/>
      <xsd:element minOccurs="0" name="SortedBy"
        type="xsd:string"/>
      <xsd:element name="HasTechnicalInstallation" type="xsd:boolean"/>
      <xsd:element minOccurs="0" name="Owner" type="xsd:string"/>
      <xsd:element minOccurs="0" name="FirmwareModuleType"
        type="tns:FirmwareModuleFilterType"/>
      <xsd:element minOccurs="0" name="FirmwareModuleVersion"
        type="xsd:string"/>
      <xsd:element minOccurs="0" name="ExactMatch"
        type="xsd:boolean"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0"
        name="DeviceIdentificationsToUse" type="xsd:string"/>
      <xsd:element maxOccurs="unbounded"
        minOccurs="0" name="DeviceIdentificationsToExclude" type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Organisation">
    <xsd:sequence>
      <xsd:element name="OrganisationIdentification" type="common:Identification"/>
      <xsd:element name="Name" type="xsd:string"/>
      <xsd:element name="Prefix" type="xsd:string"/>
      <xsd:element name="FunctionGroup" type="tns:PlatformFunctionGroup"/>
      <xsd:element name="Enabled" type="xsd:boolean"/>
      <xsd:element maxOccurs="unbounded" name="Domains"
        type="tns:PlatformDomain"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Event">
    <xsd:sequence>
      <xsd:element name="deviceIdentification" type="common:Identification"/>
      <xsd:element name="timestamp" type="xsd:dateTime"/>
      <xsd:element name="eventType" type="tns:EventType"/>
      <xsd:element name="description" type="xsd:string"/>
      <xsd:element minOccurs="0" name="index" type="xsd:int"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="DeviceOutputSetting">
    <xsd:sequence>
      <xsd:element name="internalId" type="xsd:int"/>
      <xsd:element name="externalId" type="xsd:int"/>
      <xsd:element minOccurs="0" name="alias" type="xsd:string"/>
      <xsd:element name="relayType" type="tns:RelayType"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Ean">
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1" name="code">
        <xsd:simpleType>
          <xsd:restriction base="xsd:integer">
            <xsd:maxInclusive value="999999999999999999"/>
            <xsd:minInclusive value="100000000000000000"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="description" type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="DeviceAuthorization">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="functionGroup"
        type="xsd:string"/>
      <xsd:element minOccurs="0" name="organisation"
        type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="DeviceModel">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Manufacturer"
        type="tns:Manufacturer"/>
      <xsd:element maxOccurs="1" minOccurs="0" name="ModelCode">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="255"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0"
        name="Description">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="255"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Firmware">
    <xsd:sequence>
      <xsd:element name="Id" type="xsd:int"/>
      <xsd:element minOccurs="0" name="Filename"
        type="xsd:string"/>
      <xsd:element maxOccurs="1" minOccurs="0"
        name="CreationTime" type="xsd:dateTime"/>
      <xsd:element minOccurs="0" name="Manufacturer">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="DeviceModel"
        type="tns:DeviceModel"/>
      <xsd:element maxOccurs="1" minOccurs="0"
        name="Description">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="255"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="PushToNewDevices" type="xsd:boolean"/>
      <xsd:element name="ModuleVersionComm" type="xsd:string"/>
      <xsd:element name="ModuleVersionFunc" type="xsd:string"/>
      <xsd:element name="ModuleVersionMa" type="xsd:string"/>
      <xsd:element name="ModuleVersionMbus" type="xsd:string"/>
      <xsd:element name="ModuleVersionSec" type="xsd:string"/>
      <xsd:element minOccurs="0" name="File" type="xsd:base64Binary"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Manufacturer">
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1" name="Name">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="50"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1"
        name="ManufacturerId">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1"
        name="UsePrefix" type="xsd:boolean"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="Certification">
    <xsd:sequence>
      <xsd:element maxOccurs="1" minOccurs="1"
        name="certificateDomain">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="100"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="1"
        name="certificateUrl">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="255"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:element name="FindOrganisationRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element maxOccurs="1" minOccurs="1"
          name="OrganisationIdentification" type="common:Identification"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="FindOrganisationResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element maxOccurs="1" minOccurs="0"
          name="Organisation" type="tns:Organisation"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="FindAllOrganisationsRequest">
    <xsd:complexType>
      <xsd:sequence>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="FindAllOrganisationsResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element maxOccurs="unbounded" name="Organisations"
          type="tns:Organisation"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SetEventNotificationsRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="DeviceIdentification" type="common:Identification"/>
        <xsd:element maxOccurs="8" minOccurs="0"
          name="EventNotifications" type="tns:EventNotificationType"/>
      </xsd:sequence>
    </xsd:complexType>
    <xsd:unique name="UniqueEventNotifications">
      <xsd:field xpath="."/>
      <xsd:selector xpath="tns:EventNotifications"/>
    </xsd:unique>
  </xsd:element>
  <xsd:element name="SetEventNotificationsResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="1" name="Result"
          type="common:OsgpResultType"/>
        <xsd:element name="Description" type="xsd:string"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SetEventNotificationsAsyncResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="AsyncResponse" type="common:AsyncResponse"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SetEventNotificationsAsyncRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="AsyncRequest" type="common:AsyncRequest"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="FindEventsRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" name="DeviceIdentification"
          type="common:Identification"/>
        <xsd:element minOccurs="0" name="Description" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
              Filters events with exactly the description as
              given here. If used in
              combination with DescriptionStartsWith an
              or clause will be used for combining the two criteria.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element minOccurs="0" name="DescriptionStartsWith" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
              Filters events on the start of the description as
              given here. If used in
              combination with Description an
              or clause will be used for combining the two criteria.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element minOccurs="0" name="From"
          type="xsd:dateTime"/>
        <xsd:element minOccurs="0" name="Until"
          type="xsd:dateTime"/>
        <xsd:element minOccurs="0" name="PageSize"
          type="xsd:int"/>
        <xsd:element minOccurs="0" name="Page" type="xsd:int"/>
        <xsd:element maxOccurs="unbounded" minOccurs="0"
          name="EventTypes" type="tns:EventType"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="FindEventsResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element maxOccurs="unbounded" name="Events"
          type="tns:Event"/>
        <xsd:element name="Page" type="common:Page"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="FindDevicesRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" name="PageSize"
          type="xsd:int"/>
        <xsd:element minOccurs="0" name="Page" type="xsd:int"/>
        <xsd:element minOccurs="0" name="DeviceFilter"
          type="tns:DeviceFilter"/>
        <xsd:element minOccurs="0" name="UsePages"
          type="xsd:boolean"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="FindDevicesResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element maxOccurs="unbounded" name="Devices"
          type="tns:Device"/>
        <xsd:element name="Page" type="common:Page"/>
        <xsd:element name="Message" type="xsd:string"/>
        <xsd:element name="Argument" type="xsd:string"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="FindScheduledTasksRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element maxOccurs="1" minOccurs="0"
          name="DeviceIdentification" type="common:Identification"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="FindScheduledTasksResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element maxOccurs="unbounded" name="ScheduledTask"
          type="tns:ScheduledTask"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="UpdateDeviceRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="DeviceIdentification" type="common:Identification"/>
        <xsd:element name="UpdatedDevice" type="tns:UpdatedDevice"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="UpdateDeviceResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="AsyncResponse" type="common:AsyncResponse"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SetDeviceAliasRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="DeviceIdentification" type="common:Identification"/>
        <xsd:element name="DeviceAlias" type="xsd:string"/>
        <xsd:element maxOccurs="unbounded" minOccurs="0"
          name="DeviceOutputSettings" type="tns:DeviceOutputSetting"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SetDeviceAliasResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="AsyncResponse" type="common:AsyncResponse"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SetMaintenanceStatusRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="DeviceIdentification" type="common:Identification"/>
        <xsd:element name="Status" type="xsd:boolean"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="UpdateDeviceSslCertificationRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="DeviceIdentification" type="common:Identification"/>
        <xsd:element maxOccurs="1" minOccurs="1"
          name="Certification" type="tns:Certification"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="UpdateDeviceSslCertificationResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="1" name="Result"
          type="common:OsgpResultType"/>
        <xsd:element name="Description" type="xsd:string"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="UpdateDeviceSslCertificationAsyncRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="AsyncRequest" type="common:AsyncRequest"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="UpdateDeviceSslCertificationAsyncResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="AsyncResponse" type="common:AsyncResponse"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SetDeviceVerificationKeyRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="DeviceIdentification" type="common:Identification"/>
        <xsd:element name="VerificationKey" type="tns:VerificationKey"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SetDeviceVerificationKeyResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="1" name="Result"
          type="common:OsgpResultType"/>
        <xsd:element name="Description" type="xsd:string"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SetDeviceVerificationKeyAsyncRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="AsyncRequest" type="common:AsyncRequest"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SetDeviceVerificationKeyAsyncResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="AsyncResponse" type="common:AsyncResponse"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SetMaintenanceStatusResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element maxOccurs="1" minOccurs="1"
          name="Result" type="common:OsgpResultType"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SetDeviceLifecycleStatusRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="DeviceIdentification" type="common:Identification"/>
        <xsd:element maxOccurs="1" minOccurs="1"
          name="DeviceLifecycleStatus" type="tns:DeviceLifecycleStatus"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SetDeviceLifecycleStatusResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="1" name="Result"
          type="common:OsgpResultType"/>
        <xsd:element name="Description" type="xsd:string"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SetDeviceLifecycleStatusAsyncResponse">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="common:AsyncResponse"></xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="SetDeviceLifecycleStatusAsyncRequest">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="common:AsyncRequest"></xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="UpdateDeviceCdmaSettingsRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="DeviceIdentification" type="common:Identification"/>
        <xsd:element maxOccurs="1" minOccurs="0" name="MastSegment" type="xsd:string"/>
        <xsd:element maxOccurs="1" minOccurs="0" name="BatchNumber" type="xsd:short"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="UpdateDeviceCdmaSettingsResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="1" name="Result"
          type="common:OsgpResultType"/>
        <xsd:element name="Description" type="xsd:string"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="UpdateDeviceCdmaSettingsAsyncResponse">
